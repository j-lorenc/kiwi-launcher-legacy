{
  "name": "launcher",
  "version": "1.0.0",
  "description": "universal game launcher",
  "main": "./dist/main/main.js",
  "scripts": {
    "start-renderer-dev": "webpack-dev-server --config config/webpack/renderer/webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config config/webpack/preload/webpack.preload.config.js && webpack --config config/webpack/main/webpack.main.config.js && electron ./dist/main/main.js",
    "start": "cross-env START_HOT=1 npm run start-renderer-dev",
    "test": "jest --projects config/jest/jest-renderer.test.config.js --watchAll --verbose",
    "e2e": "jest --projects config/jest/jest-main.test.config.js --watchAll --verbose",
    "test:all": "jest --watchAll --verbose",
    "build-main": "cross-env NODE_ENV=production webpack --config config/webpack/main/webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config config/webpack/renderer/webpack.renderer.prod.config.js",
    "build-preload": "cross-env NODE_ENV=production webpack --config config/webpack/preload/webpack.preload.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "rebuild": "electron-rebuild",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-lorenc/kiwi-launcher.git"
  },
  "author": "Jonathan Lorenc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-lorenc/kiwi-launcher/issues"
  },
  "homepage": "https://github.com/j-lorenc/kiwi-launcher#readme",
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@types/classnames": "^2.2.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mousetrap": "^1.6.3",
    "@types/react": "^16.9.36",
    "@types/react-dom": "^16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/react-transition-group": "^4.4.0",
    "@types/steamid": "^1.1.0",
    "@types/uuid": "^8.0.0",
    "@types/winreg": "^1.2.30",
    "classnames": "^2.2.6",
    "eslint": "^7.2.0",
    "keytar": "^6.0.1",
    "mousetrap": "^1.6.5",
    "react": "^16.13.1",
    "react-anime": "^3.0.3",
    "react-dom": "^16.13.1",
    "steamid": "^1.1.3",
    "uuid": "^8.1.0",
    "vdf": "0.0.2",
    "winreg": "^1.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@fortawesome/fontawesome-pro": "^5.13.0",
    "@jest-runner/electron": "^3.0.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/react": "^10.3.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^26.0.0",
    "@types/node-sass": "^4.11.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron": "^22.3.25",
    "electron-builder": "^22.7.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-store": "^5.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "feather-icons": "^4.28.0",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "react-draggable": "^4.4.3",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "react-transition-group": "^4.4.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^1.0.0",
    "spectron": "^11.0.0",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "build": {
    "productName": "Kiwi Launcher",
    "appId": "org.kiwi-launcher",
    "mac": {
      "category": "public.app-category.utilities"
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    }
  }
}
